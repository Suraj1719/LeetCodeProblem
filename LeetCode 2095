Problem number : (2095) Delete the Middle Node of a Linked List
//java code
class Solution {
    public ListNode deleteMiddle(ListNode head) {
        
        ListNode cur=head;int size=0;
        while(cur!=null){
            size++;
            cur=cur.next;
        }
        if(size==1)
            return null;
        if(size==2){
            head.next=null;
            return head;
        }
            
        int mid=-1;
        mid=size/2;
        ListNode temp=head;
        int pos=1;
        while(pos<mid){
            pos++;
            temp=temp.next;
        }
        temp.next=temp.next.next;
        
        return head;
        
    }
}
